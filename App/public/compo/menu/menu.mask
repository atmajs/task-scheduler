	
.nav.nav-pills.nav-stacked {
	
	var list = ['tasks','active','history','workers'];
	
	for (name of list) {
		
		li name='~[name]' > a.viewTitle x-dynamic href='/~[name]' {
			span.badge.pull-right > '~[: count[name] ]'
			
			.roll-link >
				span data-title='~[name]'  > '~[L: "nav_" + name]'
		}
	}
}
